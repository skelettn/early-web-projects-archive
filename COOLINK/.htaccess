RewriteEngine on

Options +FollowSymlinks


RewriteCond %{REQUEST_FILENAME}.php -f
RewriteRule ^(.*)$ $1.php [L]

# RewriteRule ^account/profile/(\d+)$ account/profile/?id=$1 [L]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^account/profile/([^/]+)$ account/profile/index.php?unique_id=$1 [L]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^([^/]+)$ pages.php?pseudo=$1 [L]

